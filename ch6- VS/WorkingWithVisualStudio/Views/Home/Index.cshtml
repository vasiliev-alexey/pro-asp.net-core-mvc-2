@model IEnumerable<Product> 
@{
    ViewData["Title"] = "Working with Visual Studio";
}


<table>
    <thead>
    <tr>
        <td>Name</td>
        <td >Price</td >
    </tr>
    </thead >
    <tbody>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.Price</td>
        </tr>
    }
    
    </tbody>
</table>